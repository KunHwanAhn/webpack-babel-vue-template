{
  "name": "webpack-babel-vue-template",
  "version": "0.0.1",
  "description": "Webpack + Babel + Vue.js Template without VueCli",
  "main": "src/main.js",
  "repository": "git@github.com:KunHwanAhn/webpack-babel-vue-template.git",
